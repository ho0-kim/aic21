{
    "data":{
        "train_json" : "data/train-tracks.json",
        "test_query_json" : "data/test-queries.json",
        "test_track_json" : "data/test-tracks.json",

        "cityflow_path" : ".",

        "positive_threshold" : 0.5,
        "crop_size" : [128, 128],
        "frame_size" : [256, 256],

        "skip_frame" : 4
    },

    "model":{
        "bert_pretrained_model" : "bert-base-cased",
        "embedding_size" : 512,

        "reducedim_method_frame" : "simple",
        "reducedim_method_crop" : "simple",

        "vid_bert_params" : {
            "vocab_size_or_config_json_file": 10,
            "hidden_size": 512,
            "num_hidden_layers": 4,
            "num_attention_heads": 4,
            "intermediate_size": 3072,
            "hidden_act": "gelu",
            "hidden_dropout_prob": 0.1,
            "attention_probs_dropout_prob": 0.1,
            "max_position_embeddings": 256,
            "type_vocab_size": 1,
            "initializer_range": 0.02,
            "layer_norm_eps": 1e-12
        }
    },

    "train":{
        "batch_size" : 5,
        "num_workers" : 2
    },

    "seed" : 777
}